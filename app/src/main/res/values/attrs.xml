<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="clearableEditTextStyle" format="reference" />

    <declare-styleable name="ClearableEditText">
        <attr name="cet_title" format="string"/>
        <attr name="cet_type" format="enum">
            <enum name="text" value="0" />
            <enum name="select" value="1" />
            <enum name="number" value="2" />
        </attr>
        <attr name="cet_suppress_dialog" format="boolean"/>
        <attr name="cet_edittext_bg" format="color" />
        <attr name="android:maxLines"/>
        <attr name="android:hint"/>
        <attr name="android:textColorHint" />
        <attr name="android:textColor"/>
        <attr name="android:textSize"/>
        <attr name="android:inputType"/>
        <attr name="android:digits"/>
        <attr name="android:imeOptions"/>
    </declare-styleable>
</resources>